<?xml version="1.0"?>
<launch>

  <!-- Run the map server -->
   
  <!-- <include file="$(find ucar_controller)/launch/base_driver.launch" > </include> -->
  <!--<remap from="/cmd_vel" to="/cmd_vel111" />-->
<!-- 
  <include file="$(find ydlidar)/launch/ydlidar.launch" > </include> -->
<!-- ekf -->
 <!-- <include file="$(find robot_pose_ekf)/launch/robot_pose_ekf.launch"/>-->

  <node name="map_server" pkg="map_server" type="map_server" args="$(find ucar_nav)/maps/mapname_24.yaml" output="screen">
   <param name="frame_id" value="map" />
  </node> 

 
  <!--Launch the move base with time elastic band-->
<!-- 
  <include file="$(find ucar_nav)/launch/config/amcl/amcl_omni.launch" > </include>  -->

 <!-- <node pkg="tf" type="static_transform_publisher" name="map_odom_br" args="0 0 0 0 0 0 /map /odom 100"/>-->
  <include file="$(find ucar_nav)/launch/config/amcl/amcl_omni.launch" > </include>
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find ucar_nav)/launch/config/move_base/teb_fast/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base/teb_fast/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base/teb_fast/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base/teb_fast/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find ucar_nav)/launch/config/move_base/teb_fast/teb_local_planner_params.yaml" command="load" />
    
    <param name="base_global_planner" value="global_planner/GlobalPlanner" />
    <!--<param name="neutral_cost" value="220" />-->
    <param name="planner_frequency" value="3.0" />
    <param name="planner_patience" value="5.0" />
    <param name="use_dijkstra" value="true" />
    
    <param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
    <param name="controller_frequency" value="20.0" />
    <param name="controller_patience" value="15.0" />

    <param name="clearing_rotation_allowed" value="false" />
  </node>

  

</launch>
